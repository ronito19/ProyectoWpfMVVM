<UserControl x:Class="WpfMVVM_Project.Views.PaginaPrincipal"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfMVVM_Project.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      >

    <StackPanel x:Name="paginaPrincipal" Visibility="Visible">
        
        <StackPanel.Background>
            <ImageBrush ImageSource="optica.png"/>
        </StackPanel.Background>
        <TextBox Text="SuperLentes" BorderBrush="White" FontSize="30" FontFamily="Reem Kufi" FontWeight="Bold" Margin="20" TextAlignment="Center"/>

    </StackPanel>
</UserControl>
